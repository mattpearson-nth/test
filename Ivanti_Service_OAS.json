swagger: '2.0'
info:
  title: Ivanti Service
  version: '1.0'
  description: Proxy that allows users to perform select operations in Ivanti using a REST API.
host: nthrivetest.azure-api.net
basePath: /IvantiService
schemes:
  - https
  
paths:
  /incident:
    post:
      operationId: CreateIncident
      description: Creates an incident with the provided values.
      parameters:
        - x-amf-mediaType: application/json
          in: body
          name: generated
          schema:
            example:
              Customer: test@ivantiServiceTest.com
              Summary: Test summary from the Ivanti Service
              Description: Test description from the Ivanti Service
              Service: Software
              Category: Server
              Urgency: 3 Has Workaround
              Impact: 3 Single User
              Status: Active
              Team: Service Desk
              Owner: teamMember@ivantiServiceTest.com
              Resolution: Test resolution from the Ivanti Service
              CauseCode: Install Request
              ActualService: Software
              ActualCategory: Workstation
            type: object
            additionalProperties: true
            required:
              - Customer
              - Summary
              - Description
              - Service
              - Urgency
              - Impact
              - Status
            properties:
              Customer:
                description: The email of the person who submitted the incident.
                type: string
              Summary:
                description: A summary of the incident.
                type: string
              Description:
                description: |-
                  The description of the incident with relevant details.

                   HTML formatting in this field will be visible on the incdient.
                type: string
              Service:
                description: |-
                  The affected service.

                  Accepted values:

                  * Access

                  * Hardware 

                  * ISSO/Security

                  * Messaging

                  * Network and Connectivity

                  * Software

                  *  Support and Training
                type: string
              Category:
                description: |-
                  Required when Service is Software. The category. 

                  Accepted values:

                  * Server

                  * Workstation
                type: string
              Urgency:
                description: |-
                  Specifies the time appropriateness required. Urgency and impact together define the priority value.

                  **Submitting an incident with urgency as "1 Work Stoppage" and impact as "1 Company Wide" is not allowed through the Ivanti Service.**

                  Accepted values:

                  * 1 Work Stoppage

                  * 2 Affects Daily Duties

                  * 3 Has Workaround
                type: string
              Impact:
                description: |-
                  Specifies the impact to the organization, group, or individual. Impact and urgency together define the priority value.

                  **Submitting an incident with urgency as "1 Work Stoppage" and impact as "1 Company Wide" is not allowed through the Ivanti Service.**

                  Accepted values:

                  * 1 Company Wide

                  * 2 Team

                  * 3 Single User
                type: string
              Status:
                description: |-
                  The status of the incident.

                  Accepted values:

                  * Logged

                  * Active

                  * Waiting for Customer

                  * Waiting for 3rd Party

                  * Waiting for Resolution

                  * Resolved

                  * Closed
                type: string
              Team:
                description: |-
                  Optional. The team to work on the incident. The list of valid teams is constantly updated. The team must appear in the Team dropdown in Ivanti.

                  Sample values:

                  * Active Directory

                  * Architecture

                  * Azure

                  * Backup\Restore
                type: string
              Owner:
                description: Required when an incident is set to Resolved or Closed. The email address of owner of the incident. The person must be a member of the selected Team.
                type: string
              Resolution:
                description: Required when an incident is set to Resolved or Closed. The resolution information.
                type: string
              CauseCode:
                description: |-
                  Required when an incident is set to Resolved or Closed. The cause of the incident.

                  Accepted values:

                  * Configuration

                  * Documentation Request

                  * Hardware

                  * Install Request

                  * Installation

                  * Linked Problem

                  * Other

                  * Reference Request

                  * Session Reset

                  * Software

                  * Training
                type: string
              ActualService:
                description: |-
                  The actual affected service.

                  Accepted values:

                  * Access

                  * Hardware 

                  * ISSO/Security

                  * Messaging

                  * Network and Connectivity

                  * Software

                  *  Support and Training
                type: string
              ActualCategory:
                description: |-
                  Required when Service is Software. The actual category. 

                  Accepted values:

                  * Server

                  * Workstation
                type: string
      responses:
        '201':
          description: ''
          x-amf-mediaType: application/json
          schema:
            example:
              Result: Success
              RecId: '123456789101112131415'
              IncidentNumber: '10000'
            type: object
            additionalProperties: true
            required:
              - Result
              - RecId
              - IncidentNumber
            properties:
              Result:
                description: The result of the API call
                example: Success
                type: string
              RecId:
                description: The incident's unique identifier
                example: '123456789101112131415'
                type: string
              IncidentNumber:
                description: The incident number
                example: '10000'
                type: string
        '400':
          description: ''
          x-amf-mediaType: application/json
          schema:
            example:
              Result: Error
              Detail: Failed to create incident - Customer is not an active Ivanti user
            type: object
            additionalProperties: true
            required:
              - Result
              - Detail
            properties:
              Result:
                description: The result of the API call
                example: Error
                type: string
              Detail:
                description: The error message
                example: Failed to create incident - Customer is not an active Ivanti user
                type: string
    get:
      operationId: GetIncident
      description: Fetches incidents based on the defined search values.
      parameters:
        - name: Customer
          required: false
          in: query
          type: string
        - name: Service
          required: false
          in: query
          type: string
        - name: Category
          required: false
          in: query
          type: string
        - name: Urgency
          required: false
          in: query
          type: string
        - name: Impact
          required: false
          in: query
          type: string
        - name: Status
          required: false
          in: query
          type: string
        - name: Team
          required: false
          in: query
          type: string
        - name: Owner
          required: false
          in: query
          type: string
        - name: CauseCode
          required: false
          in: query
          type: string
        - name: ActualService
          required: false
          in: query
          type: string
        - name: ActualCategory
          required: false
          in: query
          type: string
        - name: IncidentNumber
          required: false
          in: query
          type: string
        - name: FirstCallResolution
          required: false
          in: query
          type: boolean
      responses:
        '200':
          description: ''
          x-amf-mediaType: application/json
          schema:
            example:
              incidents:
                - RecId: '123456789101112131415'
                  Customer: test@ivantiServiceTest.com
                  Summary: Test summary from the Ivanti Service
                  Description: Test description from the Ivanti Service
                  Service: Software
                  Category: Server
                  Urgency: 3 Has Workaround
                  Impact: 3 Single User
                  Priority: '5'
                  Status: Active
                  Team: Service Desk
                  Owner: testTeamMember@ivantiServiceTest.com
                  Resolution: Test resolution from the Ivanti Service
                  CauseCode: Install Request
                  ActualService: Software
                  ActualCategory: Workstation
                  ClosedBy: test@ivantiServiceTest.com
                  ClosedDateTime: '2020-03-26T20:17:29Z'
                  CreatedBy: test@ivantiServiceTest.com
                  CreatedDateTime: '2020-03-26T20:17:29Z'
                  FirstCallResolution: false
                  IncidentNumber: '10125'
            type: object
            additionalProperties: true
        '204':
          description: ''
          x-amf-mediaType: application/json
          schema: {}
        '400':
          description: ''
          x-amf-mediaType: application/json
          schema:
            example:
              Result: Error
              Detail: 'Invalid search term: Customer'
